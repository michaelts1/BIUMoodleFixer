{
	"files": [
		"src/index.ts"
	],
	"include": [
		"./declaration.d.ts"
	],
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "Bundler",
		"noEmit": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": false,
		"skipLibCheck": true,
		"strict": true,
		"target": "es2024",
	},
}
